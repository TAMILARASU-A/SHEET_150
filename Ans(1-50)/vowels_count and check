string=input("Enter a string:")
#string=string.lower()
vow=[]
cons=[] 
vowels="aeiouAEIOU"
v_count=0
c_count=0
for s in string:
  if s.isalpha():
    if s in vowels:
        vow.append(s)
        v_count+=1  
    else:
        cons.append(s)
        c_count+=1
print("Vowels:",vow,"Consonats:",cons)
print("Vowels count:",v_count,"Consonants count:",c_count)
